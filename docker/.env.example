# macOS: 5000 kann belegt sein (AirPlay). Default hier 5001.
MLFLOW_PORT=5001

# Suppress MLflow startup warnings (file-based storage is default for local dev)
MLFLOW_BACKEND_STORE_URI=
MLFLOW_DEFAULT_ARTIFACT_ROOT=

# ============================================================================
# S3 Cloud Backup Configuration
# ============================================================================
# S3 credentials for cloud backup
S3_ACCESS_KEY=your_access_key
S3_SECRET_KEY=your_secret_key
S3_BUCKET_NAME=peak-trade-backups
S3_ENDPOINT_URL=  # For MinIO: http://localhost:9000
S3_REGION=us-east-1
