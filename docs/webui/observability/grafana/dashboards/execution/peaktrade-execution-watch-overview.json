{
  "uid": "peaktrade-execution-watch-overview",
  "title": "Peak_Trade — Execution Watch Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "peak_trade",
    "observability",
    "execution_watch",
    "watch_only"
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "links": [
    {
      "title": "Overview",
      "type": "dashboards",
      "tags": [],
      "asDropdown": false,
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "url": "d/peaktrade-overview/peaktrade-overview"
    },
    {
      "title": "System Health",
      "type": "dashboards",
      "tags": [],
      "asDropdown": false,
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "url": "d/peaktrade-system-health/peaktrade-system-health"
    },
    {
      "title": "Shadow Pipeline (MVS)",
      "type": "dashboards",
      "tags": [],
      "asDropdown": false,
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "url": "d/peaktrade-shadow-pipeline-mvs/peaktrade-shadow-pipeline-mvs"
    },
    {
      "title": "Execution Watch Overview",
      "type": "dashboards",
      "tags": [],
      "asDropdown": false,
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "url": "d/peaktrade-execution-watch-overview/peaktrade-execution-watch-overview"
    },
    {
      "title": "HTTP (Labeled Local)",
      "type": "dashboards",
      "tags": [],
      "asDropdown": false,
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "url": "d/peaktrade-labeled-local/peaktrade-labeled-local"
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROM",
        "label": "Prometheus",
        "type": "datasource",
        "query": "prometheus",
        "refresh": 1,
        "hide": 0,
        "multi": false,
        "includeAll": false,
        "current": {
          "text": "prometheus-local",
          "value": "prometheus-local"
        },
        "options": []
      },
      {
        "type": "datasource",
        "name": "DS_SHADOW",
        "label": "Prometheus (shadow)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-shadow",
          "value": "peaktrade-prometheus-shadow"
        },
        "options": [],
        "refresh": 1
      },
      {
        "type": "datasource",
        "name": "DS_MAIN",
        "label": "Prometheus (main)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-main",
          "value": "peaktrade-prometheus-main"
        },
        "options": [],
        "refresh": 1
      },
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-local",
          "value": "peaktrade-prometheus-local"
        },
        "options": [],
        "refresh": 1
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Health invariants (shadow)",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "UP (job=shadow_mvs)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"shadow_mvs\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Exporter (shadow_mvs_up)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(shadow_mvs_up)"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Contract: pipeline events metric present",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "((absent(peak_trade_pipeline_events_total) OR on() vector(0)))"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "PRESENT"
                },
                "1": {
                  "text": "MISSING"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Contract: execution watch metrics present",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "((absent(peak_trade_execution_watch_requests_total) OR on() vector(0)))"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "PRESENT"
                },
                "1": {
                  "text": "MISSING"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Shadow pipeline signals (throughput, errors, latency, risk blocks)",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Pipeline events rate (by stage)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 7
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (mode, stage) (rate(peak_trade_pipeline_events_total{job=\"shadow_mvs\"}[5m]))",
          "legendFormat": "{{mode}} {{stage}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Pipeline error rate",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 7
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_pipeline_events_total{job=\"shadow_mvs\",stage=\"error\"}[5m])) / clamp_min(sum(rate(peak_trade_pipeline_events_total{job=\"shadow_mvs\"}[5m])), 1e-9)",
          "legendFormat": "error_rate"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Risk blocks rate (by reason)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (mode, reason) (rate(peak_trade_risk_blocks_total{job=\"shadow_mvs\"}[5m]))",
          "legendFormat": "{{mode}} {{reason}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (intent → ack)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, mode) (rate(peak_trade_pipeline_latency_seconds_bucket{job=\"shadow_mvs\",edge=\"intent_to_ack\"}[5m])))",
          "legendFormat": "p95 {{mode}}"
        }
      ]
    },
    {
      "type": "row",
      "title": "Replay/compare signals (Execution Watch API metrics, if enabled)",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 23
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Execution Watch req/s (by endpoint, status)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (endpoint, status) (rate(peak_trade_execution_watch_requests_total[5m]))",
          "legendFormat": "{{endpoint}} {{status}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Execution Watch latency p95 (by endpoint)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 33
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(peak_trade_execution_watch_request_latency_seconds_bucket[5m])))",
          "legendFormat": "p95 {{endpoint}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Execution Watch JSONL read errors (rate)",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 33
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(peak_trade_execution_watch_read_errors_total[5m])",
          "legendFormat": "read_errors/s"
        }
      ]
    },
    {
      "type": "row",
      "title": "Compare (shadow vs main) — best-effort",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 42
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "UP (shadow) — job=peak_trade_web",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 43
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "UP (main) — job=peak_trade_web",
      "datasource": "${DS_PROM}",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 43
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    }
  ]
}
