{
  "uid": "peaktrade-execution-watch-overview",
  "title": "Peak_Trade — Execution Watch Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["peak_trade", "observability", "execution_watch", "watch_only"],
  "time": { "from": "now-30m", "to": "now" },
  "links": [
    {
      "type": "link",
      "title": "Operator Home",
      "url": "/d/peaktrade-operator-home",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Contract Details",
      "url": "/d/peaktrade-contract-details",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Shadow Pipeline (MVS)",
      "url": "/d/peaktrade-shadow-pipeline-mvs",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "HTTP Labeled (Local)",
      "url": "/d/peaktrade-labeled-local",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Overview (Mini vs Main)",
      "url": "/d/peaktrade-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_SHADOW",
        "label": "Prometheus (shadow)",
        "query": "prometheus",
        "current": { "text": "prometheus-shadow", "value": "peaktrade-prometheus-shadow" },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_MAIN",
        "label": "Prometheus (main)",
        "query": "prometheus",
        "current": { "text": "prometheus-main", "value": "peaktrade-prometheus-main" },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": { "text": "prometheus-local", "value": "peaktrade-prometheus-local" },
        "options": [],
        "refresh": 1,
        "hide": 2
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "name": "Reject markers",
        "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
        "expr": "(sum(increase(peaktrade_ai_decisions_total{decision=\"reject\"}[1m])) or on() vector(0)) > 0",
        "enable": true,
        "iconColor": "red",
        "tags": ["reject"]
      },
      {
        "name": "Accept markers",
        "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
        "expr": "(sum(increase(peaktrade_ai_decisions_total{decision=\"accept\"}[1m])) or on() vector(0)) > 0",
        "enable": true,
        "iconColor": "green",
        "tags": ["accept"]
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "AI Live",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "AI Active (last 30s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_max(((sum(increase(peaktrade_ai_decisions_total[30s])) > bool 0) or on() vector(0)), 1)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 0.5 }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": { "0": { "text": "INACTIVE" }, "1": { "text": "ACTIVE" } }
            }
          ]
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Total decisions / min (1m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(rate(peaktrade_ai_decisions_total[1m]) * 60) or on() vector(0))"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0, "decimals": 1 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Reject share (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(((sum(increase(peaktrade_ai_decisions_total{decision=\"reject\"}[5m])) or on() vector(0)) / clamp_min((sum(increase(peaktrade_ai_decisions_total[5m])) or on() vector(0)), 1e-9)), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "orange", "value": 0.3 },
              { "color": "red", "value": 0.5 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Last decision age (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(time() - (max(peaktrade_ai_last_decision_timestamp_seconds) or on() vector(0)), 0)",
          "legendFormat": "age_s"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "min": 0, "color": { "mode": "fixed" } }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Decisions / min (1m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peaktrade_ai_decisions_total{decision=\"accept\"}[1m]) * 60) or on() vector(0)",
          "legendFormat": "accept"
        },
        {
          "refId": "B",
          "expr": "sum(rate(peaktrade_ai_decisions_total{decision=\"reject\"}[1m]) * 60) or on() vector(0)",
          "legendFormat": "reject"
        },
        {
          "refId": "C",
          "expr": "sum(rate(peaktrade_ai_decisions_total{decision=\"noop\"}[1m]) * 60) or on() vector(0)",
          "legendFormat": "noop"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "stacking": { "group": "A", "mode": "normal" }, "fillOpacity": 30 }
        },
        "overrides": []
      }
    },
    {
      "type": "bargauge",
      "title": "Top reject reasons (10m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 13 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(8, sum by (reason) (increase(peaktrade_ai_decisions_total{decision=\"reject\"}[10m])))",
          "legendFormat": "{{reason}}"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0 }, "overrides": [] },
      "options": { "displayMode": "gradient", "orientation": "horizontal", "showUnfilled": true }
    },
    {
      "type": "row",
      "title": "Health invariants (shadow)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 19 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "UP (job=shadow_mvs)",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 20 },
      "targets": [
        { "refId": "A", "expr": "min(up{job=\"shadow_mvs\"})" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Exporter (shadow_mvs_up)",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 20 },
      "targets": [
        { "refId": "A", "expr": "max(shadow_mvs_up)" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Contract: pipeline events metric present",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 20 },
      "targets": [
        { "refId": "A", "expr": "((absent(peak_trade_pipeline_events_total) OR on() vector(0)))" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "PRESENT" }, "1": { "text": "MISSING" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 1 }
            ]
          },
          "links": [
            {
              "title": "Contract Details",
              "url": "/d/peaktrade-contract-details?var-metric=peak_trade_pipeline_events_total",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Contract: execution watch metrics present",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 20 },
      "targets": [
        {
          "refId": "A",
          "expr": "((absent(peak_trade_execution_watch_requests_total) OR on() vector(0)))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "PRESENT" }, "1": { "text": "MISSING" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 1 }
            ]
          },
          "links": [
            {
              "title": "Contract Details",
              "url": "/d/peaktrade-contract-details?var-metric=peak_trade_execution_watch_requests_total",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Shadow pipeline signals (throughput, errors, latency, risk blocks)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 25 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Pipeline events rate (by stage)",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 26 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (mode, stage) (rate(peak_trade_pipeline_events_total{job=\"shadow_mvs\"}[5m]))",
          "legendFormat": "{{mode}} {{stage}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Pipeline error rate",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 26 },
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 1 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_pipeline_events_total{job=\"shadow_mvs\",stage=\"error\"}[5m])) / clamp_min(sum(rate(peak_trade_pipeline_events_total{job=\"shadow_mvs\"}[5m])), 1e-9)",
          "legendFormat": "error_rate"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Risk blocks rate (by reason)",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 34 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (mode, reason) (rate(peak_trade_risk_blocks_total{job=\"shadow_mvs\"}[5m]))",
          "legendFormat": "{{mode}} {{reason}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (intent → ack)",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 34 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, mode) (rate(peak_trade_pipeline_latency_seconds_bucket{job=\"shadow_mvs\",edge=\"intent_to_ack\"}[5m])))",
          "legendFormat": "p95 {{mode}}"
        }
      ]
    },
    {
      "type": "row",
      "title": "Replay/compare signals (Execution Watch API metrics, if enabled)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 42 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Execution Watch req/s (by endpoint, status)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 43 },
      "fieldConfig": {
        "defaults": {
          "links": [
            {
              "title": "Drilldown: Execution Watch Details",
              "url": "/d/peaktrade-execution-watch-details?var-endpoint=${__series.labels.endpoint}&var-status=${__series.labels.status}",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (endpoint, status) (rate(peak_trade_execution_watch_requests_total[5m]))",
          "legendFormat": "{{endpoint}} {{status}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Execution Watch latency p95 (by endpoint)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 52 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(peak_trade_execution_watch_request_latency_seconds_bucket[5m])))",
          "legendFormat": "p95 {{endpoint}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Execution Watch JSONL read errors (rate)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 52 },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(peak_trade_execution_watch_read_errors_total[5m])",
          "legendFormat": "read_errors/s"
        }
      ]
    },
    {
      "type": "row",
      "title": "Compare (shadow vs main) — best-effort",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 61 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "UP (shadow) — job=peak_trade_web",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 62 },
      "targets": [
        { "refId": "A", "expr": "up{job=\"peak_trade_web\"}", "legendFormat": "{{instance}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "UP (main) — job=peak_trade_web",
      "datasource": { "type": "prometheus", "uid": "${DS_MAIN}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 62 },
      "targets": [
        { "refId": "A", "expr": "up{job=\"peak_trade_web\"}", "legendFormat": "{{instance}}" }
      ]
    }
  ]
}
