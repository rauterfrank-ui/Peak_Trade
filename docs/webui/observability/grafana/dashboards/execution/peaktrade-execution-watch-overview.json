{
  "uid": "peaktrade-execution-watch-overview",
  "title": "Peak_Trade — Execution Watch Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "10s",
  "tags": ["peak_trade", "observability", "execution_watch", "watch_only"],
  "time": { "from": "now-30m", "to": "now" },
  "links": [
    {
      "type": "link",
      "title": "Operator Home",
      "url": "/d/peaktrade-operator-home",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Contract Details",
      "url": "/d/peaktrade-contract-details",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Shadow Pipeline (MVS)",
      "url": "/d/peaktrade-shadow-pipeline-mvs",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "HTTP Labeled (Local)",
      "url": "/d/peaktrade-labeled-local",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Overview (Mini vs Main)",
      "url": "/d/peaktrade-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_SHADOW",
        "label": "Prometheus (shadow)",
        "query": "prometheus",
        "current": { "text": "prometheus-shadow", "value": "peaktrade-prometheus-shadow" },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_MAIN",
        "label": "Prometheus (main)",
        "query": "prometheus",
        "current": { "text": "prometheus-main", "value": "peaktrade-prometheus-main" },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": { "text": "prometheus-local", "value": "peaktrade-prometheus-local" },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "query",
        "name": "run_id",
        "label": "AI run_id",
        "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
        "query": { "query": "label_values(peaktrade_ai_decisions_total, run_id)", "refId": "runIdVars" },
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "refresh": 2,
        "sort": 1,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "name": "Reject markers",
        "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
        "expr": "(sum(increase(peaktrade_ai_decisions_total{decision=\"reject\",run_id=~\"$run_id\"}[1m])) or on() vector(0)) > 0",
        "enable": true,
        "iconColor": "red",
        "tags": ["reject"]
      },
      {
        "name": "Accept markers",
        "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
        "expr": "(sum(increase(peaktrade_ai_decisions_total{decision=\"accept\",run_id=~\"$run_id\"}[1m])) or on() vector(0)) > 0",
        "enable": true,
        "iconColor": "green",
        "tags": ["accept"]
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "AI Live — Ops Summary",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 64 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "AI Live Up",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 65 },
      "targets": [
        {
          "refId": "A",
          "expr": "(max(up{job=\"ai_live\"}) or on() vector(0))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } }
            }
          ]
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "AI Live Fresh (worst age s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 65 },
      "targets": [
        {
          "refId": "A",
          "expr": "max(clamp_min(time() - max by (source)(peaktrade_ai_last_event_timestamp_seconds_by_run_id{run_id=~\"$run_id\"}), 0)) or on() vector(0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 120 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Parse errors / 5m",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 65 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(increase(peaktrade_ai_events_parse_errors_total[5m])) or on() vector(0))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Drops / 5m",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 65 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(increase(peaktrade_ai_events_dropped_total[5m])) or on() vector(0))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Latency p95 (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 65 },
      "targets": [
        {
          "refId": "A",
          "expr": "(histogram_quantile(0.95, sum(rate(peaktrade_ai_decision_latency_ms_bucket[5m])) by (le)) or on() vector(0))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 250 },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Active alerts (firing)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 65 },
      "targets": [
        {
          "refId": "A",
          "expr": "(count(ALERTS{alertstate=\"firing\"}) or on() vector(0))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "row",
      "title": "AI Live",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "AI Active (last 30s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_max(((sum(increase(peaktrade_ai_decisions_total{run_id=~\"$run_id\"}[30s])) > bool 0) or on() vector(0)), 1)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 0.5 }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": { "0": { "text": "INACTIVE" }, "1": { "text": "ACTIVE" } }
            }
          ]
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Total decisions / min (1m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(rate(peaktrade_ai_decisions_total{run_id=~\"$run_id\"}[1m]) * 60) or on() vector(0))"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0, "decimals": 1 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Reject share (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(((sum(increase(peaktrade_ai_decisions_total{decision=\"reject\",run_id=~\"$run_id\"}[5m])) or on() vector(0)) / clamp_min((sum(increase(peaktrade_ai_decisions_total{run_id=~\"$run_id\"}[5m])) or on() vector(0)), 1e-9)), 0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "orange", "value": 0.3 },
              { "color": "red", "value": 0.5 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Last decision age (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(time() - (max(peaktrade_ai_last_decision_timestamp_seconds{run_id=~\"$run_id\"}) or on() vector(0)), 0)",
          "legendFormat": "age_s"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "min": 0, "color": { "mode": "fixed" } }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Recent: last decision age (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 5 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(time() - (max(peaktrade_ai_last_decision_timestamp_seconds{run_id=~\"$run_id\"}) or on() vector(0)), 0)"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "min": 0 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Recent: last event age (s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 5 },
      "targets": [
        {
          "refId": "A",
          "expr": "max(clamp_min(time() - max by (source) (peaktrade_ai_last_event_timestamp_seconds_by_run_id{run_id=~\"$run_id\"}), 0)) or on() vector(0)"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "min": 0 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Recent: decisions (1m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 5 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(increase(peaktrade_ai_decisions_total{run_id=~\"$run_id\"}[1m])) or on() vector(0))"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Recent: decisions (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 5 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(increase(peaktrade_ai_decisions_total{run_id=~\"$run_id\"}[5m])) or on() vector(0))"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Decisions / min (1m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 9 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peaktrade_ai_decisions_total{decision=\"accept\",run_id=~\"$run_id\"}[1m]) * 60) or on() vector(0)",
          "legendFormat": "accept"
        },
        {
          "refId": "B",
          "expr": "sum(rate(peaktrade_ai_decisions_total{decision=\"reject\",run_id=~\"$run_id\"}[1m]) * 60) or on() vector(0)",
          "legendFormat": "reject"
        },
        {
          "refId": "C",
          "expr": "sum(rate(peaktrade_ai_decisions_total{decision=\"noop\",run_id=~\"$run_id\"}[1m]) * 60) or on() vector(0)",
          "legendFormat": "noop"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "stacking": { "group": "A", "mode": "normal" }, "fillOpacity": 30 }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "AI Live — UX v2 (reasons, actions, SLO, timeline)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Decisions by decision (1m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum(increase(peaktrade_ai_decisions_total{decision=\"accept\",run_id=~\"$run_id\"}[1m])) or on() vector(0))",
          "legendFormat": "accept"
        },
        {
          "refId": "B",
          "expr": "(sum(increase(peaktrade_ai_decisions_total{decision=\"reject\",run_id=~\"$run_id\"}[1m])) or on() vector(0))",
          "legendFormat": "reject"
        },
        {
          "refId": "C",
          "expr": "(sum(increase(peaktrade_ai_decisions_total{decision=\"noop\",run_id=~\"$run_id\"}[1m])) or on() vector(0))",
          "legendFormat": "noop"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "stacking": { "group": "A", "mode": "normal" }, "fillOpacity": 30 }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Actions (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(6, sum by (action) (increase(peaktrade_ai_actions_total{run_id=~\"$run_id\"}[5m]))) OR on(action) label_replace(vector(0), \"action\", \"none\", \"\", \"\")",
          "legendFormat": "{{action}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "stacking": { "group": "A", "mode": "normal" }, "fillOpacity": 30 }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Reject reasons (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 26 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(6, sum by (reason) (increase(peaktrade_ai_decisions_total{decision=\"reject\",run_id=~\"$run_id\"}[5m]))) OR on(reason) label_replace(vector(0), \"reason\", \"none\", \"\", \"\")",
          "legendFormat": "{{reason}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "stacking": { "group": "A", "mode": "normal" }, "fillOpacity": 30 }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Noop reasons (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 26 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(6, sum by (reason) (increase(peaktrade_ai_decisions_total{decision=\"noop\",run_id=~\"$run_id\"}[5m]))) OR on(reason) label_replace(vector(0), \"reason\", \"none\", \"\", \"\")",
          "legendFormat": "{{reason}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "custom": { "stacking": { "group": "A", "mode": "normal" }, "fillOpacity": 30 }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Latency SLO > 500ms (5m) — breach %",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 34 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(((sum(rate(peaktrade_ai_decision_latency_seconds_bucket{le=\"+Inf\",run_id=~\"$run_id\"}[5m])) > bool 0) * (1 - (sum(rate(peaktrade_ai_decision_latency_seconds_bucket{le=\"0.5\",run_id=~\"$run_id\"}[5m])) / clamp_min(sum(rate(peaktrade_ai_decision_latency_seconds_bucket{le=\"+Inf\",run_id=~\"$run_id\"}[5m])), 1e-9))) * 100), 0) or on() vector(0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Latency breach % (>500ms) (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 34 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(((sum(rate(peaktrade_ai_decision_latency_seconds_bucket{le=\"+Inf\",run_id=~\"$run_id\"}[5m])) > bool 0) * (1 - (sum(rate(peaktrade_ai_decision_latency_seconds_bucket{le=\"0.5\",run_id=~\"$run_id\"}[5m])) / clamp_min(sum(rate(peaktrade_ai_decision_latency_seconds_bucket{le=\"+Inf\",run_id=~\"$run_id\"}[5m])), 1e-9))) * 100), 0) or on() vector(0)",
          "legendFormat": "breach_pct"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent", "min": 0, "max": 100 }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "AI Activity State (per decision type, last 30m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 42 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_max(((sum(increase(peaktrade_ai_decisions_total{decision=\"accept\",run_id=~\"$run_id\"}[1m])) > bool 0) or on() vector(0)), 1)",
          "legendFormat": "accept_active"
        },
        {
          "refId": "B",
          "expr": "clamp_max(((sum(increase(peaktrade_ai_decisions_total{decision=\"reject\",run_id=~\"$run_id\"}[1m])) > bool 0) or on() vector(0)), 1)",
          "legendFormat": "reject_active"
        },
        {
          "refId": "C",
          "expr": "clamp_max(((sum(increase(peaktrade_ai_decisions_total{decision=\"noop\",run_id=~\"$run_id\"}[1m])) > bool 0) or on() vector(0)), 1)",
          "legendFormat": "noop_active"
        }
      ],
      "fieldConfig": {
        "defaults": { "min": 0, "max": 1, "custom": { "fillOpacity": 80 } },
        "overrides": []
      }
    },
    {
      "type": "bargauge",
      "title": "Top reject reasons (10m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 48 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(8, sum by (reason) (increase(peaktrade_ai_decisions_total{decision=\"reject\",run_id=~\"$run_id\"}[10m]))) OR on(reason) label_replace(vector(0), \"reason\", \"none\", \"\", \"\")",
          "legendFormat": "{{reason}}"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0 }, "overrides": [] },
      "options": { "displayMode": "gradient", "orientation": "horizontal", "showUnfilled": true }
    },
    {
      "type": "stat",
      "title": "Latency p95 (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 54 },
      "targets": [
        {
          "refId": "A",
          "expr": "(histogram_quantile(0.95, sum(rate(peaktrade_ai_decision_latency_ms_bucket[5m])) by (le)) or on() vector(0))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ms", "min": 0 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Parse errors / min (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 54 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peaktrade_ai_events_parse_errors_total[5m])) or on() vector(0)"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0 }, "overrides": [] },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "bargauge",
      "title": "Drops / min (5m)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 54 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peaktrade_ai_events_dropped_total[5m])) by (reason)",
          "legendFormat": "{{reason}}"
        }
      ],
      "fieldConfig": { "defaults": { "min": 0 }, "overrides": [] },
      "options": { "displayMode": "gradient", "orientation": "horizontal", "showUnfilled": true }
    },
    {
      "type": "timeseries",
      "title": "Source freshness (age s)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 58 },
      "targets": [
        {
          "refId": "A",
          "expr": "clamp_min(time() - max by (source) (peaktrade_ai_last_event_timestamp_seconds), 0)",
          "legendFormat": "{{source}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "min": 0 }, "overrides": [] }
    },
    {
      "type": "row",
      "title": "Health invariants (shadow)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 69 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "UP (job=shadow_mvs)",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 70 },
      "targets": [
        { "refId": "A", "expr": "min(up{job=\"shadow_mvs\"})" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Exporter (shadow_mvs_up)",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 70 },
      "targets": [
        { "refId": "A", "expr": "max(shadow_mvs_up)" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Contract: pipeline events metric present",
