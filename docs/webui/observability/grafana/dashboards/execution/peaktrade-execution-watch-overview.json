        { "refId": "A", "expr": "((absent(peak_trade_pipeline_events_total) OR on() vector(0)))" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "PRESENT" }, "1": { "text": "MISSING" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 1 }
            ]
          },
          "links": [
            {
              "title": "Contract Details",
              "url": "/d/peaktrade-contract-details?var-metric=peak_trade_pipeline_events_total",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Contract: execution watch metrics present",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 70 },
      "targets": [
        {
          "refId": "A",
          "expr": "((absent(peak_trade_execution_watch_requests_total) OR on() vector(0)))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "PRESENT" }, "1": { "text": "MISSING" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 1 }
            ]
          },
          "links": [
            {
              "title": "Contract Details",
              "url": "/d/peaktrade-contract-details?var-metric=peak_trade_execution_watch_requests_total",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Shadow pipeline signals (throughput, errors, latency, risk blocks)",
