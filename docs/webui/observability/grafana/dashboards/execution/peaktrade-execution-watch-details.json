  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-local",
          "value": "peaktrade-prometheus-local"
        },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_MAIN",
        "label": "Prometheus (main)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-main",
          "value": "peaktrade-prometheus-main"
        },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_SHADOW",
        "label": "Prometheus (shadow)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-shadow",
          "value": "peaktrade-prometheus-shadow"
        },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "query",
        "name": "endpoint",
        "label": "endpoint",
        "datasource": {
          "type": "prometheus",
          "uid": "peaktrade-prometheus-local"
        },
        "query": {
          "query": "label_values(peak_trade_execution_watch_requests_total, endpoint)",
          "refId": "endpointVars"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "refresh": 2,
        "sort": 1,
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "type": "query",
        "name": "status",
        "label": "status",
        "datasource": {
          "type": "prometheus",
          "uid": "peaktrade-prometheus-local"
        },
        "query": {
          "query": "label_values(peak_trade_execution_watch_requests_total, status)",
          "refId": "statusVars"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "refresh": 2,
        "sort": 1,
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "text",
      "title": "Drilldown context",
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "options": {
        "mode": "markdown",
        "content": "Diese View ist für Drilldowns aus **Execution Watch Overview**.\n\n- Filter: `endpoint=~\"$endpoint\"`, `status=~\"$status\"`\n- Datasource ist **DS_LOCAL** (Guardrail gegen false MISSING auf DS_SHADOW)."
      }
    },
    {
      "type": "timeseries",
      "title": "Execution Watch req/s (filtered)",
      "datasource": {
        "type": "prometheus",
        "uid": "peaktrade-prometheus-local"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (endpoint, status) (rate(peak_trade_execution_watch_requests_total{endpoint=~\"$endpoint\",status=~\"$status\"}[5m]))",
          "legendFormat": "{{endpoint}} {{status}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Execution Watch latency p95 (filtered)",
      "datasource": {
        "type": "prometheus",
        "uid": "peaktrade-prometheus-local"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(peak_trade_execution_watch_request_latency_seconds_bucket{endpoint=~\"$endpoint\"}[5m])))",
          "legendFormat": "p95 {{endpoint}}"
        }
      ]
    }
  ],
  "timepicker": {
    "hidden": false
  }
}
