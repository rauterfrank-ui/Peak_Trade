    {
      "type": "link",
      "title": "Explore (req/s, endpoint/status)",
      "url": "/explore?left=%7B%22datasource%22%3A%22$%7BDS_LOCAL%7D%22%2C%22queries%22%3A%5B%7B%22refId%22%3A%22A%22%2C%22expr%22%3A%22sum%20by%20%28endpoint%2C%20status%29%20%28rate%28peak_trade_execution_watch_requests_total%7Bendpoint%3D~%5C%22$endpoint%5C%22%2Cstatus%3D~%5C%22$status%5C%22%7D%5B5m%5D%29%29%22%7D%5D%2C%22range%22%3A%7B%22from%22%3A%22now-30m%22%2C%22to%22%3A%22now%22%7D%7D",
      "targetBlank": false,
