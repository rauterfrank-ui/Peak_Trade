{
  "uid": "peaktrade-main-vs-shadow-overview",
  "title": "Peak_Trade — Compare (Main vs Shadow) Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["peak_trade", "observability", "compare", "main", "shadow", "watch-only", "compare_pack_v0_1"],
  "time": { "from": "now-30m", "to": "now" },
  "links": [
    { "type": "link", "title": "Operator Home", "url": "/d/peaktrade-operator-home", "targetBlank": false, "keepTime": true, "includeVars": true },
    { "type": "link", "title": "System Health (Operator)", "url": "/d/peaktrade-system-health", "targetBlank": false, "keepTime": true, "includeVars": true },
    { "type": "link", "title": "Shadow Pipeline (MVS)", "url": "/d/peaktrade-shadow-pipeline-mvs", "targetBlank": false, "keepTime": true, "includeVars": true },
    { "type": "link", "title": "Execution Watch Overview", "url": "/d/peaktrade-execution-watch-overview", "targetBlank": false, "keepTime": true, "includeVars": true },
    { "type": "link", "title": "Metrics Drift (Compare)", "url": "/d/peaktrade-metrics-drift", "targetBlank": false, "keepTime": true, "includeVars": true }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": { "text": "prometheus-local", "value": "peaktrade-prometheus-local" },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_MAIN",
        "label": "Prometheus (main)",
        "query": "prometheus",
        "current": { "text": "prometheus-main", "value": "peaktrade-prometheus-main" },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_SHADOW",
        "label": "Prometheus (shadow)",
        "query": "prometheus",
        "current": { "text": "prometheus-shadow", "value": "peaktrade-prometheus-shadow" },
        "options": [],
        "refresh": 1,
        "hide": 2
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Scrape health (up) — MAIN vs SHADOW",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "UP (peak_trade_web) — shadow",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 5, "w": 12, "x": 0, "y": 1 },
      "targets": [{ "refId": "A", "expr": "min(up{job=\"peak_trade_web\"})" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": 0 }, { "color": "green", "value": 1 }] }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "UP (peak_trade_web) — main (optional)",
      "datasource": { "type": "prometheus", "uid": "${DS_MAIN}" },
      "gridPos": { "h": 5, "w": 12, "x": 12, "y": 1 },
      "targets": [{ "refId": "A", "expr": "min(up{job=\"peak_trade_web\"})" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": 0 }, { "color": "green", "value": 1 }] }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Contract present (best-effort) — MAIN vs SHADOW",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Contract: http requests metric present — shadow",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 5, "w": 12, "x": 0, "y": 7 },
      "targets": [{ "refId": "A", "expr": "((absent(peak_trade_http_requests_total) OR on() vector(0)))" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "type": "value", "options": { "0": { "text": "PRESENT" }, "1": { "text": "MISSING" } } }],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": 0 }, { "color": "red", "value": 1 }] },
          "links": [{ "title": "Contract Details", "url": "/d/peaktrade-contract-details?var-metric=peak_trade_http_requests_total", "targetBlank": false }]
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Contract: http requests metric present — main (optional)",
      "datasource": { "type": "prometheus", "uid": "${DS_MAIN}" },
      "gridPos": { "h": 5, "w": 12, "x": 12, "y": 7 },
      "targets": [{ "refId": "A", "expr": "((absent(peak_trade_http_requests_total) OR on() vector(0)))" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "type": "value", "options": { "0": { "text": "PRESENT" }, "1": { "text": "MISSING" } } }],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": 0 }, { "color": "red", "value": 1 }] },
          "links": [{ "title": "Contract Details", "url": "/d/peaktrade-contract-details?var-metric=peak_trade_http_requests_total", "targetBlank": false }]
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Shadow Pipeline MVS quick compare (best-effort)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 12 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Pipeline events rate (shadow_mvs) — shadow",
      "datasource": { "type": "prometheus", "uid": "${DS_SHADOW}" },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 13 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_pipeline_events_total{job=\"shadow_mvs\"}[5m]))",
          "legendFormat": "shadow total"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Pipeline events (main) — N/A by default",
      "description": "Main typically does not export shadow_mvs pipeline contract metrics. If you have main exporting them, replace this panel with a real main query.",
      "datasource": { "type": "prometheus", "uid": "${DS_MAIN}" },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 13 },
      "targets": [{ "refId": "A", "expr": "vector(0)" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "mappings": [{ "type": "value", "options": { "0": { "text": "N/A" } } }],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": 0 }] }
        },
        "overrides": []
      }
    }
  ]
}
