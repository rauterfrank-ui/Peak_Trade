{
  "uid": "peaktrade-overview",
  "title": "Peak_Trade Overview (Mini vs Main)",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "5s",
  "tags": [
    "peak_trade",
    "observability"
  ],
  "panels": [
    {
      "type": "timeseries",
      "title": "UP (mini) — job=peak_trade_web",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "peaktrade-prometheus-local"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "UP (main) — job=peak_trade_uvicorn (main prom missing)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "peaktrade-prometheus-local"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "absent(up{job=\"peak_trade_uvicorn\"})",
          "legendFormat": "main_missing_absent"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP req/s (mini) — peak_trade_http_requests_total",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "peaktrade-prometheus-local"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total[5m]))",
          "legendFormat": "req/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP req/s (main) — peak_trade_http_requests_total (main prom missing)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "peaktrade-prometheus-local"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "vector(0)",
          "legendFormat": "main_missing_0"
        }
      ]
    }
  ]
}
