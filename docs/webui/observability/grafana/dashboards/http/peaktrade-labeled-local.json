{
  "uid": "peaktrade-labeled-local",
  "title": "Peak_Trade Labeled (Local / 9092)",
  "tags": [
    "observability",
    "peak_trade",
    "labeled",
    "local"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "links": [
    {
      "type": "link",
      "title": "Operator Home",
      "url": "/d/peaktrade-operator-home",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Execution Watch Overview",
      "url": "/d/peaktrade-execution-watch-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "System Health",
      "url": "/d/peaktrade-system-health",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "name": "ds",
        "label": "Datasource",
        "type": "datasource",
        "query": "prometheus",
        "regex": "^(prom_local_9092|prom_shadow_9093|prom_ai_live_9094|prom_observability_9095)$",
        "refresh": 1,
        "hide": 0,
        "multi": false,
        "includeAll": false,
        "allValue": null,
        "sort": 0,
        "skipUrlSync": false,
        "current": {
          "text": "prom_local_9092",
          "value": "prom_local_9092"
        },
        "options": []
      },
      {
        "type": "query",
        "name": "route",
        "label": "route",
        "datasource": {
          "type": "prometheus",
          "uid": "${ds}"
        },
        "query": {
          "query": "label_values(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}, route)",
          "refId": "routeVars"
        },
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "RPS (labeled, all routes)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}[5m]))",
          "legendFormat": "total"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Avg latency (s) (labeled, all routes)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_request_duration_seconds_labeled_sum{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}[5m])) / sum(rate(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}[5m]))",
          "legendFormat": "avg"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "RPS by route (labeled)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (route) (rate(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\",route=~\"$route\"}[5m]))",
          "legendFormat": "{{route}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "In-flight requests (labeled)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(peak_trade_http_in_flight_requests_labeled{service=\"peak_trade_web\",env=\"local\",stack=\"host\"})",
          "legendFormat": "in_flight"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "stat",
      "title": "up{job=peak_trade_web} (local)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 18
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    }
  ],
  "timepicker": {
    "hidden": false
  }
}
