{
  "uid": "peaktrade-labeled-local",
  "title": "Peak_Trade Labeled (Local / 9092)",
  "tags": ["observability", "peak_trade", "labeled", "local"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-15m", "to": "now" },
  "links": [
    {
      "type": "link",
      "title": "Overview (Local vs Main)",
      "url": "/d/peaktrade-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Shadow Pipeline (MVS)",
      "url": "/d/peaktrade-shadow-pipeline-mvs",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Execution Watch Overview",
      "url": "/d/peaktrade-execution-watch-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": { "text": "prometheus-local", "value": "peaktrade-prometheus-local" },
        "options": [],
        "refresh": 1
      },
      {
        "type": "query",
        "name": "route",
        "label": "route",
        "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
        "query": {
          "query": "label_values(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}, route)",
          "refId": "routeVars"
        },
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "RPS (labeled, all routes)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}[5m]))",
          "legendFormat": "total"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Avg latency (s) (labeled, all routes)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_request_duration_seconds_labeled_sum{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}[5m])) / sum(rate(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\"}[5m]))",
          "legendFormat": "avg"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "RPS by route (labeled)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (route) (rate(peak_trade_http_requests_total_labeled_total{service=\"peak_trade_web\",env=\"local\",stack=\"host\",route=~\"$route\"}[5m]))",
          "legendFormat": "{{route}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "In-flight requests (labeled)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 18 },
      "targets": [
        {
          "refId": "A",
          "expr": "max(peak_trade_http_in_flight_requests_labeled{service=\"peak_trade_web\",env=\"local\",stack=\"host\"})",
          "legendFormat": "in_flight"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "up{job=peak_trade_web} (local)",
      "datasource": { "type": "prometheus", "uid": "${DS_LOCAL}" },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 18 },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    }
  ]
}
