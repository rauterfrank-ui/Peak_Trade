    },
    {
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "title": "Contract Details",
      "type": "link",
      "url": "/d/peaktrade-contract-details"
    },
    {
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "title": "Overview (Mini vs Main)",
      "type": "link",
      "url": "/d/peaktrade-overview"
    },
    {
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "title": "System Health (Operator)",
      "type": "link",
      "url": "/d/peaktrade-system-health"
    },
    {
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "title": "Execution Watch Overview",
      "type": "link",
      "url": "/d/peaktrade-execution-watch-overview"
    },
    {
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "title": "HTTP Labeled (Local)",
      "type": "link",
      "url": "/d/peaktrade-labeled-local"
    },
    {
      "includeVars": true,
      "keepTime": true,
      "targetBlank": false,
      "title": "Shadow Pipeline (MVS)",
      "type": "link",
      "url": "/d/peaktrade-shadow-pipeline-mvs"
    }
  ],
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Stack Fingerprint",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "panels": []
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Stack Fingerprint: Active Stacks",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count by (stack) (up{stack=~\".+\"})",
          "instant": true,
          "range": false,
          "legendFormat": "{{stack}}"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Stack Fingerprint: Target Count",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count(up{stack=~\".+\"})",
          "instant": true,
          "range": false,
          "legendFormat": "__auto"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "table",
      "title": "Stack Fingerprint: Targets UP",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 7
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{stack=~\".+\"} == 1",
          "instant": true,
          "range": false,
          "legendFormat": "__auto"
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": []
      }
    },
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 17
      },
      "panels": [],
      "title": "Operator quicklinks (watch-only)",
      "type": "row"
    },
    {
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 18
      },
      "options": {
        "content": "Use the dashboard header links for drilldowns.\n\n**Recommended flow (watch-only):** System Health → Execution Watch → HTTP Labeled.\nShadow Pipeline (MVS) is included for quick cross-checks.",
        "mode": "markdown"
      },
      "title": "How to use",
      "type": "text"
    },
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 24
      },
      "panels": [],
      "title": "Strategy & Risk Telemetry (watch/paper/shadow)",
      "type": "row"
    },
    {
      "type": "timeseries",
      "title": "Strategy decisions rate (5m)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 25
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (strategy_id, decision) (rate(peaktrade_strategy_decisions_total[5m]))",
          "legendFormat": "{{strategy_id}} {{decision}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Strategy signals rate (5m)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 25
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (strategy_id, signal) (rate(peaktrade_strategy_signals_total[5m]))",
          "legendFormat": "{{strategy_id}} {{signal}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Risk blocks rate (5m) — by check",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 33
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (check) (rate(peaktrade_risk_checks_total{result=\"block\"}[5m]))",
          "legendFormat": "{{check}}"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Risk blocks rate (5m) — by reason",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 33
      },
      "fieldConfig": {
        "defaults": {
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(12, sum by (reason) (rate(peaktrade_risk_blocks_total[5m])))",
          "legendFormat": "{{reason}}"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Risk limit utilization (0..1)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 41
      },
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 0.8
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max by (limit_id) (peaktrade_risk_limit_utilization)",
          "legendFormat": "{{limit_id}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Strategy gross exposure (notional, abs)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 41
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (strategy_id, ccy) (peaktrade_strategy_position_gross_exposure)",
          "legendFormat": "{{strategy_id}} {{ccy}}"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": [
    "observability",
    "operator",
    "peak_trade",
    "summary",
    "watch-only"
  ],
  "templating": {
    "list": [
      {
        "name": "ds",
        "label": "Datasource",
        "type": "datasource",
        "query": "prometheus",
        "regex": "^(prom_local_9092|prom_shadow_9093|prom_ai_live_9094|prom_observability_9095)$",
        "refresh": 1,
        "hide": 0,
        "multi": false,
        "includeAll": false,
        "allValue": null,
        "sort": 0,
        "skipUrlSync": false,
        "current": {
          "text": "prom_local_9092",
          "value": "prom_local_9092"
        },
        "options": []
      }
    ]
  },
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timezone": "browser",
  "title": "PeakTrade Operator Summary",
  "uid": "peaktrade-operator-summary",
  "version": 1,
  "timepicker": {
    "hidden": false
  }
}
