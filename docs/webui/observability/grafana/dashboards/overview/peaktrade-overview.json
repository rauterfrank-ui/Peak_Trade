        "w": 12,
        "x": 12,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "peaktrade-prometheus-main"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total[5m]))",
          "legendFormat": "req/s"
        }
      ]
    }
