{
  "uid": "peaktrade-overview",
  "title": "Peak_Trade Overview (Mini vs Main)",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "10s",
  "tags": [
    "peak_trade",
    "observability"
  ],
  "links": [
    {
      "type": "link",
      "title": "Operator Home",
      "url": "/d/peaktrade-operator-home",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "System Health (Operator)",
      "url": "/d/peaktrade-system-health",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Operator Summary",
      "url": "/d/peaktrade-operator-summary",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Contract Details",
      "url": "/d/peaktrade-contract-details",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Shadow Pipeline (MVS)",
      "url": "/d/peaktrade-shadow-pipeline-mvs",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Execution Watch Overview",
      "url": "/d/peaktrade-execution-watch-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "name": "ds",
        "label": "Datasource",
        "type": "datasource",
        "query": "prometheus",
        "regex": "^(prom_local_9092|prom_shadow_9093|prom_ai_live_9094|prom_observability_9095)$",
        "refresh": 1,
        "hide": 0,
        "multi": false,
        "includeAll": false,
        "allValue": null,
        "sort": 0,
        "skipUrlSync": false,
        "current": {
          "text": "prom_local_9092",
          "value": "prom_local_9092"
        },
        "options": []
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "UP (local) — job=peak_trade_web",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "UP (main) — job=peak_trade_web",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP req/s (local) — peak_trade_http_requests_total",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total[5m]))",
          "legendFormat": "req/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP req/s (main) — peak_trade_http_requests_total",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total[5m]))",
          "legendFormat": "req/s"
        }
      ]
    }
  ],
  "timepicker": {
    "hidden": false
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}
