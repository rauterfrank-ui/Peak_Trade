{
  "uid": "peaktrade-overview",
  "title": "Peak_Trade Overview (Mini vs Main)",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "5s",
  "tags": [
    "peak_trade",
    "observability"
  ],
  "links": [
    {
      "type": "link",
      "title": "Shadow Pipeline (MVS)",
      "url": "/d/peaktrade-shadow-pipeline-mvs",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Execution Watch Overview",
      "url": "/d/peaktrade-execution-watch-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "HTTP Labeled (Local)",
      "url": "/d/peaktrade-labeled-local",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": { "text": "prometheus-local", "value": "peaktrade-prometheus-local" },
        "options": [],
        "refresh": 1
      },
      {
        "type": "datasource",
        "name": "DS_MAIN",
        "label": "Prometheus (main)",
        "query": "prometheus",
        "current": { "text": "prometheus-main", "value": "peaktrade-prometheus-main" },
        "options": [],
        "refresh": 1
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "UP (local) — job=peak_trade_web",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_LOCAL}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "UP (main) — job=peak_trade_web",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_MAIN}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"peak_trade_web\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP req/s (local) — peak_trade_http_requests_total",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_LOCAL}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total[5m]))",
          "legendFormat": "req/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP req/s (main) — peak_trade_http_requests_total",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_MAIN}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(peak_trade_http_requests_total[5m]))",
          "legendFormat": "req/s"
        }
      ]
    }
  ]
}
