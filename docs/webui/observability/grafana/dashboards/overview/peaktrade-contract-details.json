        "x": 8,
        "y": 23
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count({__name__=\"$metric\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
