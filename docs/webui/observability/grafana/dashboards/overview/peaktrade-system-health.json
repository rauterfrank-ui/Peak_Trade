{
  "uid": "peaktrade-system-health",
  "title": "Peak_Trade — System Health (Operator)",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "peak_trade",
    "observability",
    "overview",
    "operator"
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "links": [
    {
      "type": "link",
      "title": "Operator Home",
      "url": "/d/peaktrade-operator-home",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Contract Details",
      "url": "/d/peaktrade-contract-details",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Overview (Local vs Main)",
      "url": "/d/peaktrade-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Shadow Pipeline (MVS)",
      "url": "/d/peaktrade-shadow-pipeline-mvs",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Execution Watch Overview",
      "url": "/d/peaktrade-execution-watch-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_SHADOW",
        "label": "Prometheus (shadow)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-shadow",
          "value": "peaktrade-prometheus-shadow"
        },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_LOCAL",
        "label": "Prometheus (local)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-local",
          "value": "peaktrade-prometheus-local"
        },
        "options": [],
        "refresh": 1,
        "hide": 2
      },
      {
        "type": "datasource",
        "name": "DS_MAIN",
        "label": "Prometheus (main)",
        "query": "prometheus",
        "current": {
          "text": "prometheus-main",
          "value": "peaktrade-prometheus-main"
        },
        "options": [],
        "refresh": 1,
        "hide": 2
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Targets up (quick)",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "UP (shadow_mvs)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_SHADOW}"
      },
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"shadow_mvs\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "UP (peak_trade_web) — local",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_LOCAL}"
      },
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 8,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"peak_trade_web\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "UP (peak_trade_web) — main (optional)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_MAIN}"
      },
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 16,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"peak_trade_web\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Scrape health (rates)",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Scrape samples scraped (rate) — local",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_LOCAL}"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 7
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (job) (rate(scrape_samples_scraped[5m]))",
          "legendFormat": "{{job}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Scrape duration (p95) — local",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_LOCAL}"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 7
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, job) (rate(scrape_duration_seconds_bucket[5m])))",
          "legendFormat": "{{job}}"
        }
      ]
    }
  ],
  "timepicker": {
    "hidden": false
  }
}
