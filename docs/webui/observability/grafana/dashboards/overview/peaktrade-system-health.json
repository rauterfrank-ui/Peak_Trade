{
  "uid": "peaktrade-system-health",
  "title": "Peak_Trade — System Health (Operator)",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "peak_trade",
    "observability",
    "overview",
    "operator"
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "links": [
    {
      "type": "link",
      "title": "Operator Home",
      "url": "/d/peaktrade-operator-home",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Contract Details",
      "url": "/d/peaktrade-contract-details",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Overview (Local vs Main)",
      "url": "/d/peaktrade-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Shadow Pipeline (MVS)",
      "url": "/d/peaktrade-shadow-pipeline-mvs",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    },
    {
      "type": "link",
      "title": "Execution Watch Overview",
      "url": "/d/peaktrade-execution-watch-overview",
      "targetBlank": false,
      "keepTime": true,
      "includeVars": true
    }
  ],
  "templating": {
    "list": [
      {
        "name": "ds",
        "label": "Datasource",
        "type": "datasource",
        "query": "prometheus",
        "regex": "^(prom_local_9092|prom_shadow_9093|prom_ai_live_9094|prom_observability_9095)$",
        "refresh": 1,
        "hide": 0,
        "multi": false,
        "includeAll": false,
        "allValue": null,
        "sort": 0,
        "skipUrlSync": false,
        "current": {
          "text": "prom_local_9092",
          "value": "prom_local_9092"
        },
        "options": []
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Stack Fingerprint",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "panels": []
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Stack Fingerprint: Active Stacks",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count by (job) (up)",
          "instant": true,
          "range": false,
          "legendFormat": "{{stack}}"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Stack Fingerprint: Target Count",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count(up)",
          "instant": true,
          "range": false,
          "legendFormat": "__auto"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "table",
      "title": "Stack Fingerprint: Targets UP",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 7
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up == 1",
          "instant": true,
          "range": false,
          "legendFormat": "__auto"
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Targets up (quick)",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 17
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "UP (shadow_mvs)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"shadow_mvs\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "UP (peak_trade_web) — local",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 8,
        "y": 18
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"peak_trade_web\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "UP (peak_trade_web) — main (optional)",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 16,
        "y": 18
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"peak_trade_web\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Scrape health (rates)",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 23
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Scrape samples scraped (rate) — local",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (job) (rate(scrape_samples_scraped[5m]))",
          "legendFormat": "{{job}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Scrape duration (p95) — local",
      "datasource": {
        "type": "prometheus",
        "uid": "${ds}"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, job) (rate(scrape_duration_seconds_bucket[5m])))",
          "legendFormat": "{{job}}"
        }
      ]
    }
  ],
  "timepicker": {
    "hidden": false
  }
}
